ance();\n        actor.setMapper(mapper);\n\t\t*/\n\n        const renderer = fullScreenRenderer.getRenderer();\n        const renderWindow = fullScreenRenderer.getRenderWindow();\n\n        renderer.addActor(actor);\n        renderer.resetCamera();\n        renderWindow.render();\n\n        context.value = {\n          fullScreenRenderer,\n          renderWindow,\n          renderer,\n          reader,\n          //coneSource,\n          actor,\n          mapper,\n        };\n      }\n    });\n\n    onBeforeUnmount(() => {\n      if (context.value) {\n\t\tconst {fullScreenRenderer, coneSource, actor, mapper } = context.value;\n        //const { renderer, renderWindow, fullScreenRenderer, coneSource, actor, mapper } = context.value;\n        actor.delete();\n        mapper.delete();\n        coneSource.delete();\n\t\t\n\t\t//renderer.delete()\n\t\t//renderWindow.delete()\n\t\t\n        fullScreenRenderer.delete();\n        context.value = null;\n      }\n    });\n\n    return {\n      vtkContainer,\n      setRepresentation,\n      setConeResolution,\n      coneResolution,\n      representation,\n    };\n  }\n}\n</script>\n\n<style scoped>\n.controls {\n  position: absolute;\n  top: 25px;\n  left: 25px;\n  background: white;\n  padding: 12px;\n}\n</style>"],"mappings":";;EAESA,GAAG,EAAC;AAAc;;EAChBC,KAAK,EAAC;AAAU;mBAH3B;mBAAA;;uBACEC,mBAAA,CA8BM,cA7BJC,mBAAA,CAA0B,OAA1BC,UAA0B,+BAC1BD,mBAAA,CA2BQ,SA3BRE,UA2BQ,GA1BNF,mBAAA,CAyBQ,gBAxBNA,mBAAA,CAYK,aAXHA,mBAAA,CAUK,aATHA,mBAAA,CAQS;IAPPG,KAAmB,EAAnB;MAAA;IAAA,CAAmB;IAClBC,KAAK,EAAEC,MAAA,CAAAC,cAAc;IACrBC,QAAM,EAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAAEJ,MAAA,CAAAK,iBAAiB,CAACD,MAAM,CAACE,MAAM,CAACP,KAAK;gCAE9CJ,mBAAA,CAAiC;IAAzBI,KAAK,EAAC;EAAG,GAAC,QAAM,qBACxBJ,mBAAA,CAAoC;IAA5BI,KAAK,EAAC;EAAG,GAAC,WAAS,qBAC3BJ,mBAAA,CAAkC;IAA1BI,KAAK,EAAC;EAAG,GAAC,SAAO,oB,mCAdvCQ,UAAA,E,KAkBQZ,mBAAA,CAUK,aATHA,mBAAA,CAQK,aAPHA,mBAAA,CAME;IALAa,IAAI,EAAC,OAAO;IACZC,GAAG,EAAC,GAAG;IACPC,GAAG,EAAC,IAAI;IACPX,KAAK,EAAEC,MAAA,CAAAW,cAAc;IAC